<%= render 'stores/store_managing_menu'%>

<h1>Products</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Price</th>
  </tr>

<% @products.each do |product| %>
  <tr>
    <%= image_tag product.photo.url(:small) %>
    <td><%= product.title %></td>
    <td><%= product.description %></td>
    <td><%= product.price %></td>

  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product', new_store_product_path %>
